{
  "$schema": "./docs/schemas/config.schema.json",
  "version": "1.0.0",

  "git": {
    "proxies": [
      "https://gitclone.com/github.com/{repo}",
      "https://gh-proxy.com/github.com/{repo}"
    ],
    "ssl_verify": false
  },

  "raw": {
    "proxies": [
      "https://ghfast.top/https://raw.githubusercontent.com/{path}"
    ]
  },
  "pip": {
    "mirrors": [],
    "index_url": null,
    "extra_index_url": null
  },

  "network": {
    "timeout": 120,
    "retry": 3
  }
}
